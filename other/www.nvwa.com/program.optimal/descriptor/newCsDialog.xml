<?xml version="1.0" encoding="utf-8" ?>
<form startPosition="CenterParent"
      caption="新建文件"
      borderStyle ="FixedToolWindow">

  <size width="539" height="350" />

  <controls>

    <__t virstream="panel"
         controlName="panel1"
         autoScroll="false"
         dockStyle="Bottom">

      <size height="40" />

      <controls>

        <__t virstream="button"
             controlName="ok"
             text="确定"
             onClick="url://www.nvwa.com/library/program\program.optimal:program.optimal.NewFileOkCommand">

          <point x="360" y="9" />

        </__t>

        <__t virstream="button"
             controlName="cancel"
             text="取消"
             onClick="url://www.nvwa.com/library/program\program.optimal:program.optimal.NewFileCancelCommand">

          <point x="445" y="9" />

        </__t>

        <__t virstream="textLabel"
             controlName="textLabel1"
             text="文件名称：">

          <point x="8" y="16" />
          <size width="66" height="12" />

        </__t>

        <__t virstream="textBox"
             controlName="fileName"
             default="test">

          <point x="74" y="10" />
          <size width="279" height="21" />

        </__t>

      </controls>

    </__t>

    <__t virstream="panel"
         controlName="panel2"
         autoScroll="false"
         dockStyle="Fill">

      <size height="71" />

      <controls>

        <__t virstream="listView"
             controlName="listView1"
             viewStyle="false"
             dockStyle="Fill" />

        <__t virstream="treeView"
             controlName="treeView1"
             showRootLines="true"
             dockStyle="Left"
             initCommand="url://www.nvwa.com/library/program\program.optimal:program.optimal.TreeViewInitCsCommand">

          <size width="160" />

        </__t>

      </controls>

    </__t>

  </controls>

</form>
